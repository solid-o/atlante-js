{
  "name": "@solido/atlante-js",
  "version": "0.4.6",
  "description": "JS Http Client",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc npm test",
    "coverage-html": "nyc --reporter=html npm test",
    "lint": "eslint lib/",
    "lint-fix": "eslint --fix lib/",
    "test": "node ./tests.cjs"
  },
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "dependencies": {
    "@jymfony/contracts": "https://github.com/jymfony/contracts.git",
    "@jymfony/exceptions": "https://github.com/jymfony/exceptions.git",
    "@jymfony/util": "https://github.com/jymfony/util.git"
  },
  "devDependencies": {
    "@jymfony/autoloader": "https://github.com/jymfony/autoloader.git",
    "@jymfony/compiler": "next",
    "@jymfony/datetime": "https://github.com/jymfony/date-time.git",
    "@jymfony/debug": "https://github.com/jymfony/debug.git",
    "@jymfony/testing": "https://github.com/jymfony/testing.git",
    "@jymfony/var-dumper": "https://github.com/jymfony/var-dumper.git",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "axios": "^1.4",
    "dom-storage": "^2.1",
    "eslint": "^8.42.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "typescript": "^5.1.3",
    "whatwg-url": "^13.0.0"
  },
  "config": {
    "jymfony-autoload": {
      "namespaces": {
        "Solido.Atlante": "lib"
      }
    },
    "jymfony-autoload-dev": {
      "namespaces": {
        "Solido.Atlante.Stubs": "stubs/",
        "Solido.Atlante.Tests": "test/"
      }
    }
  },
  "nyc": {
    "per-file": true,
    "hookRequire": true,
    "hookRunInContext": true,
    "hookRunInThisContext": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/tests.js",
      "**/namespace-stub.js",
      "**/fixtures/**",
      "**/test/**"
    ],
    "reporter": [
      "text-summary",
      "text",
      "clover"
    ],
    "all": true,
    "report-dir": "./coverage"
  }
}
